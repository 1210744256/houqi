        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baizhi.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.baizhi.entity.User">
